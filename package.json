{
  "name": "twillio-sms-mock",
  "version": "1.0.0",
  "description": "Local Twillio API mock server for SMS testing with TypeScript support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "twilio",
    "sms",
    "mock",
    "testing",
    "typescript",
    "api",
    "development",
    "sdk"
  ],
  "author": "Daniel Georgiev <daniel@dabanny.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:dgeorgiev/twillio-sms-mock.git"
  },
  "bugs": {
    "url": "https://github.com/dgeorgiev/twillio-sms-mock/issues"
  },
  "homepage": "https://github.com/dgeorgiev/twillio-sms-mock",
  "dependencies": {
    "express": "^4.18.2",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}